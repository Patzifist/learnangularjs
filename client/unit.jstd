load:
  - src/test/webapp/js/lib/unit/jasmine.js
  - src/test/webapp/js/lib/unit/JasmineAdapter.js
  - src/main/webapp/js/lib/angular.js
  - src/main/webapp/js/lib/angular-resource.js
