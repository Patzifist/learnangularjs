load:
  - src/main/webapp/js/lib/jquery/jquery.js
  - src/test/webapp/js/lib/unit/jasmine/jasmine.js
  - src/test/webapp/js/lib/unit/angular/JasmineAdapter.js
  - src/main/webapp/js/lib/angular/angular.js
  - src/main/webapp/js/lib/angular/angular-resource.js
  - src/test/webapp/js/lib/unit/angular/angular-mocks.js
  - src/test/webapp/js/lib/unit/jsonpath/jsonpath.js
  - src/main/webapp/js/app/*.js

test:
  - src/test/webapp/js/unit/*-spec.js

plugin:
  - name: "testisolation"
    jar: "support/testisolation-nodeps-1.3.3d.jar"
    module: "com.google.jstestdriver.plugins.testisolation.TestIsolationInitializer"

